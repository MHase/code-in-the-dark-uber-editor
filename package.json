{
  "name": "electron-webpack-quick-start",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .tsx,.ts,.js,.jsx src/",
    "stylelint": "stylelint 'src/**/*.scss'",
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "test": "jest --outputFile test-results.json --json",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@material-ui/core": "^4.3.2",
    "classnames": "^2.2.6",
    "formik": "^1.5.8",
    "react": "^16.9.0",
    "react-ace": "^7.0.2",
    "react-dom": "^16.9.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "source-map-support": "^0.5.12",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@codechecks/build-size-watcher": "^0.1.0",
    "@codechecks/client": "^0.1.6",
    "@testing-library/react": "^9.1.3",
    "@types/classnames": "^2.2.9",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.4",
    "@types/yup": "^0.26.22",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "danger": "^9.1.8",
    "danger-plugin-jest": "^1.3.0",
    "danger-plugin-yarn": "^1.3.2",
    "electron": "5.0.6",
    "electron-builder": "^21.0.11",
    "electron-webpack": "^2.7.4",
    "electron-webpack-eslint": "^5.0.1",
    "electron-webpack-ts": "^3.2.0",
    "eslint": "5.16.0",
    "eslint-config-google": "^0.13.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "jest": "^24.9.0",
    "node-sass": "^4.12.0",
    "pre-commit": "^1.2.2",
    "prettier": "1.18.2",
    "sass-loader": "^7.2.0",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^3.0.1",
    "stylelint-scss": "^3.9.3",
    "typescript": "^3.5.3",
    "webpack": "~4.35.3"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(s?css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "pre-commit": [
    "test",
    "lint",
    "stylelint"
  ]
}
